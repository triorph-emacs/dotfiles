export CPPFLAGS="-I$JAVA_HOME/include"

typeset -U PATH path
path=(
  "/opt/openmpi-5.0.2/bin"
  "$HOME/.local/bin"
  "$HOME/.docker/bin"
  "$HOME/.cargo/bin"
  "$HOME/.local/share/gem/ruby/3.0.0/bin"
  "$HOME/.gem/ruby/2.6.0/bin"
  "$HOME/.config/emacs/bin"
  "$JAVA_HOME/bin"
  "$path[@]"
)
export PATH

. "$HOME/.cargo/env"
export LD_LIBRARY_PATH="/usr/local/lib:$LD_LIBRARY_PATH"
